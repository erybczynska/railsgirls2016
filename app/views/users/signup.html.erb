
<%= form_for(User.new, url: create_user_path) do |form| %>
  <p><%= form.label("first_name") %></p>
  <p><%= form.text_field("first_name") %></p>
  <p><%= form.label("last_name") %></p>
  <p><%= form.text_field("last_name") %></p>
  <p><%= form.label("username") %></p>
  <p><%= form.text_field("username") %></p>
  <p><%= form.label("password") %></p>
  <p><%= form.password_field("password") %></p>
  <p><%= form.submit %></p>
<% end %>
